{
  "name": "shogun-message-plugin",
  "version": "1.3.16",
  "description": "End-to-end encrypted messaging plugin for Shogun SDK",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "browser": "dist/browser/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "browser": "./dist/browser/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:cjs && npm run build:esm && npm run build:browser",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:browser": "webpack --mode production",
    "clean": "rimraf dist",
    "test": "jest",
    "lint": "prettier --check src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "shogun",
    "messaging",
    "encrypted",
    "private",
    "chat",
    "communication",
    "plugin"
  ],
  "author": "Shogun Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shogun-org/shogun-message-plugin.git"
  },
  "dependencies": {
    "gun": "^0.2020.1241",
    "shogun-core": "^1.9.4",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "https-browserify": "^1.0.0",
    "jest": "^29.7.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.0.3",
    "process": "^0.11.10",
    "rimraf": "^5.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.2.2",
    "url": "^0.11.3",
    "util": "^0.12.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
